import java.util.Scanner;

class Main {
  
  public static void main(String[] args) {
    
    System.out.printf("Insira o preço da gasolina: ");
    
    Scanner Var = new Scanner(System.in);
    float Gasolina = Var.nextFloat();

    System.out.printf("Insira o valor do pagamento: ");

    float Pagamento = Var.nextFloat();

    float Final = (Pagamento/Gasolina);
    
    System.out.printf("Você consegue colocar %.2f L de gasolina", Final);
    
  }
  
}
